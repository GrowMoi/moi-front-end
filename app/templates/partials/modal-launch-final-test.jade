.modal.transparent
  ion-pane.transparent
    .score-container.row
      .main-image.col
        img(src="images/user.png")
      .col-50
        .text-container
          h2#title {{ 'modal_end_test.title' | translate }} {{model.name}}!
          h2#content {{ 'modal_end_test.content' | translate }}
      .pista.col
    .controls-score
      span.btn-score.button.button-right(ng-click="model.createFinalTest()") {{ 'modal_end_test.ok' | translate }}
      span.btn-score.button.button-left(ng-click="model.closeModal()") {{ 'modal_end_test.cancel' | translate }}
