ion-view(view-title='Dashboard')
  ion-nav-buttons(side='left')
    button.button.button-icon.icon.ion-navicon(menu-toggle='left')
  ion-content.padding
    h2
      | {{ 'dashboard.welcome' | translate }},
      | {{name}}
    ion-list
      ion-item.item-remove-animate.item-avatar.item-icon-right(ng-repeat='neuron in neurons', type='item-text-wrap')
        div
          | {{neuron.title}}
