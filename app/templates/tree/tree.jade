ion-view(view-title='TreeMoi')
  ion-nav-buttons(side='left')
    button.button.button-icon.icon.ion-navicon(menu-toggle='left')
  .app-content
    a.arrow.back-arrow(href="/#/login")
      img(src="../images/back-arrow.png")
    .cointainer-cloud.animated.slideInLeft
      img.cloud1(src="../images/nube1L.png")
      img.cloud2(src="../images/nube2L.png")
      img.cloud3(src="../images/nube3L.png")
    .tree-content
      figure.flowerpot
        img.flowerpot-img(src="../images/macetaH.png", alt="maceta - tree moi")
      gif-queue.tree
        gif-no-loop.tree-img(src="../images/arbol.gif" order="0")
        gif-no-loop.tree-neuron(src="../images/bola_amarilla.gif" order="1", ng-click="treeModel.transitionToContent()")
